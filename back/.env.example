# Backend environment overrides (optional)
# Copy to .env and adjust as needed.

# Ports
WS_BRIDGE_PORT=9003
TCP_PORT=12346
OTA_PORT=12345

# WebSocket tuning
WS_PING_INTERVAL=20
WS_PING_TIMEOUT=20
WS_MAX_SIZE=2000000

# Kinematics
WHEEL_RADIUS=0.0325
MECANUM_LX=0.10
MECANUM_LY=0.10
# MECANUM_LSUM=0.20
# ROBOT_RADIUS=0.10
MECANUM_VY_SIGN=1.0
MECANUM_OMEGA_SIGN=1.0
IMU_EULER_YAW_INDEX=0

# Logging
LOG_LEVEL=INFO
LOG_DIRECTORY=logs/bridge_logs

# Bridge broadcast behavior
BRIDGE_BROADCAST_ALL_STATUS=1
BRIDGE_BROADCAST_ALL_TYPES=0
BRIDGE_BROADCAST_MIN_INTERVAL=0.05